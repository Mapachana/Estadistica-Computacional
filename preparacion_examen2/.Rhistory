determina.n <- function(epsilon, sigma, alpha){
z <- qnorm(alpha/2, lower.tail=FALSE)
n <- (z*sigma/epsilon)^2
return(n)
}
epislon <- 0.1*10
print(determina.n(epsilon, 5, 0.05))
